page{
    width: 100%;
    height: 100%;
    background: #F5F5F5;
    font-size: 14px;
}
page > view{
    margin-bottom: 30rpx;
    padding: 20rpx 30rpx;
}
.avatar-list-panel{
    background: #fff;
    display: flex;
    padding:0 0 30rpx 0;
    flex-wrap:wrap;
}
.member{
    width: 90rpx;
    padding:20rpx 30rpx 0px 30rpx;
}
.memeber-name{
    text-align: center;
    font-size:12px;
    overflow:hidden;
    text-overflow:ellipsis;
}
.member-avatar{
    width:90rpx;
    height:90rpx;
    border-radius:50%;
    border-color:#fff;
}
.member-name{
    font-size:14px;
    text-align:center;
}
.team-panel{
    background:#fff;
    display: flex;
    justify-content:space-between;
    border-bottom: 2rpx solid #F5F5F5;
}
.team-name{
   color: #666666;
}
.team-option-panel{
    background: #fff;
    padding: 0 30rpx;
}
.team-item{
    display: flex;
    justify-content:space-between;
    line-height:80rpx;
}
.team-item > switch::before{
    height: 50rpx;
    width: 20rpx;
}
.border-bottom{
    border-bottom: 2rpx solid #F5F5F5;
}
.clear-panel{
    background: #fff;
    padding-left: 30rpx;
}

/* switch大小设置 */
.team-item .wx-switch-input{width:42px;height:22px;}
.team-item .wx-switch-input::before{width:41px;height: 20px;}
.team-item .wx-switch-input::after{width: 20px;height: 20px;}
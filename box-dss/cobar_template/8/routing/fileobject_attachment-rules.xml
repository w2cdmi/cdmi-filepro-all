<rules>
    <rule>
        <namespace>fileObjectAttachment</namespace>
        <shardingExpression>hash.apply(object.objectID) % 1024 &lt; 128</shardingExpression>
        <shards>partition1</shards>
    </rule>
    <rule>
        <namespace>fileObjectAttachment</namespace>
        <shardingExpression>hash.apply(object.objectID) % 1024 &gt;= 128 and hash.apply(object.objectID) % 1024 &lt; 256</shardingExpression>
        <shards>partition2</shards>
    </rule>
    <rule>
        <namespace>fileObjectAttachment</namespace>
        <shardingExpression>hash.apply(object.objectID) % 1024 &gt;= 256 and hash.apply(object.objectID) % 1024 &lt; 384</shardingExpression>
        <shards>partition3</shards>
    </rule>
    <rule>
        <namespace>fileObjectAttachment</namespace>
        <shardingExpression>hash.apply(object.objectID) % 1024 &gt;= 384 and hash.apply(object.objectID) % 1024 &lt; 512</shardingExpression>
        <shards>partition4</shards>
    </rule>
    <rule>
        <namespace>fileObjectAttachment</namespace>
        <shardingExpression>hash.apply(object.objectID) % 1024 &gt;= 512 and hash.apply(object.objectID) % 1024 &lt; 640</shardingExpression>
        <shards>partition5</shards>
    </rule>
    <rule>
        <namespace>fileObjectAttachment</namespace>
        <shardingExpression>hash.apply(object.objectID) % 1024 &gt;= 640 and hash.apply(object.objectID) % 1024 &lt; 768</shardingExpression>
        <shards>partition6</shards>
    </rule>
    <rule>
        <namespace>fileObjectAttachment</namespace>
        <shardingExpression>hash.apply(object.objectID) % 1024 &gt;= 768 and hash.apply(object.objectID) % 1024 &lt; 896</shardingExpression>
        <shards>partition7</shards>
    </rule>
    <rule>
        <namespace>fileObjectAttachment</namespace>
        <shardingExpression>hash.apply(object.objectID) % 1024 &gt;= 896</shardingExpression>
        <shards>partition8</shards>
    </rule>
</rules>
